{
  "name": "@ts-safeql/eslint-plugin",
  "version": "3.6.0",
  "license": "MIT",
  "repository": "https://github.com/ts-safeql/safeql/tree/master/packages/eslint-plugin",
  "files": [
    "dist",
    "package.json"
  ],
  "type": "module",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "main": "dist/index.cjs",
  "exports": {
    ".": {
      "types": {
        "import": "./dist/index.d.mts",
        "require": "./dist/index.d.cts",
        "default": "./dist/index.d.ts"
      },
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./config": {
      "types": {
        "import": "./dist/config.d.mts",
        "require": "./dist/config.d.cts",
        "default": "./dist/config.d.ts"
      },
      "import": "./dist/config.mjs",
      "require": "./dist/config.cjs"
    }
  },
  "scripts": {
    "demo": "tsx src/index.ts",
    "build": "unbuild",
    "dev": "unbuild --stub",
    "test": "vitest --pool=forks",
    "lint": "eslint src",
    "lint!": "eslint src --fix",
    "clean": "rm -rf lib"
  },
  "devDependencies": {
    "@ts-safeql/generate": "workspace:*",
    "@ts-safeql/shared": "workspace:*",
    "@ts-safeql/sql-ast": "workspace:*",
    "@ts-safeql/test-utils": "workspace:*",
    "@types/node": "^20.17.10",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@typescript-eslint/rule-tester": "^7.18.0",
    "eslint": "^8.57.1",
    "libpg-query": "^16.3.0",
    "typescript": "^5.7.2",
    "unbuild": "^2.0.0",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "chokidar": "^3.6.0",
    "fp-ts": "^2.16.9",
    "pg-connection-string": "2.5.0",
    "postgres": "^3.4.5",
    "synckit": "^0.9.2",
    "ts-pattern": "^4.3.0",
    "tsx": "^4.19.2",
    "zod": "^3.24.1",
    "zod-to-json-schema": "3.20.1"
  },
  "peerDependencies": {
    "@typescript-eslint/utils": ">=7.8.0",
    "libpg-query": ">=13.2.5"
  }
}
