{
  "name": "@ts-safeql/eslint-plugin",
  "version": "3.6.12",
  "license": "MIT",
  "repository": "https://github.com/ts-safeql/safeql/tree/master/packages/eslint-plugin",
  "files": [
    "dist",
    "package.json"
  ],
  "type": "module",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "main": "dist/index.cjs",
  "exports": {
    ".": {
      "types": {
        "import": "./dist/index.d.mts",
        "require": "./dist/index.d.cts",
        "default": "./dist/index.d.ts"
      },
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./config": {
      "types": {
        "import": "./dist/config.d.mts",
        "require": "./dist/config.d.cts",
        "default": "./dist/config.d.ts"
      },
      "import": "./dist/config.mjs",
      "require": "./dist/config.cjs"
    }
  },
  "scripts": {
    "demo": "tsx src/index.ts",
    "build": "unbuild",
    "dev": "unbuild --stub",
    "test": "vitest --pool=forks --hideSkippedTests=true",
    "lint": "eslint src",
    "lint!": "eslint src --fix",
    "typecheck": "tsc -b",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "@ts-safeql/generate": "workspace:*",
    "@ts-safeql/shared": "workspace:*",
    "@ts-safeql/sql-ast": "workspace:*",
    "@ts-safeql/test-utils": "workspace:*",
    "@types/node": "^22.13.13",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@typescript-eslint/rule-tester": "^8.28.0",
    "eslint": "^9.23.0",
    "libpg-query": "^17.5.2",
    "typescript": "^5.8.2",
    "unbuild": "^3.5.0",
    "vitest": "^3.0.9"
  },
  "dependencies": {
    "chokidar": "^4.0.3",
    "fp-ts": "^2.16.9",
    "pg-connection-string": "2.7.0",
    "postgres": "^3.4.5",
    "quansync": "^0.2.10",
    "synckit": "^0.10.3",
    "ts-pattern": "^5.6.2",
    "tsx": "^4.19.3",
    "zod": "^3.24.2",
    "zod-to-json-schema": "3.24.5"
  },
  "peerDependencies": {
    "@typescript-eslint/utils": ">=8.0.0",
    "libpg-query": ">=13.2.5"
  }
}
