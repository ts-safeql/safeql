{
  "name": "@ts-safeql/shared",
  "version": "4.0.5",
  "license": "MIT",
  "repository": "https://github.com/ts-safeql/safeql/tree/master/packages/eslint-plugin",
  "files": [
    "dist",
    "package.json"
  ],
  "type": "module",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "main": "dist/index.cjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
