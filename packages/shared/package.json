    "lint": "eslint src",
    "lint!": "eslint src --fix",
    "typecheck": "tsc -b",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "@types/node": "^22.13.13",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "eslint": "^9.23.0",
    "typescript": "^5.8.2",
    "unbuild": "^3.5.0",
    "vitest": "^3.0.9"
  },
  "dependencies": {
    "@typescript-eslint/utils": "^8.28.0",
    "minimatch": "^10.0.1",
    "postgres": "^3.4.5"
  }
}